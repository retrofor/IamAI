{
  "name": "root",
  "private": true,
  "workspaces": [
    "website"
  ],
  "scripts": {
    "archive": "yarn workspace IamAI docusaurus docs:version",
    "build": "yarn workspace website build",
    "build:plugin": "cross-env BASE_URL='/website/' yarn workspace IamAI build",
    "start": "yarn workspace IamAI start",
    "serve": "yarn workspace IamAI serve",
    "clear": "yarn workspace IamAI clear",
    "prettier": "prettier --config ./.prettierrc --write \"./website/\""
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^2.5.0"
  }
}
